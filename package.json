{
  "name": "ff-tools",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "type": "module",
  "scripts": {
    "dev:server": "tsx watch src/server/index.ts",
    "dev:web": "vite --config vite.config.ts",
    "dev": "concurrently -n server,web -c blue,magenta \"pnpm:dev:server\" \"pnpm:dev:web\"",
    "build:web": "vite build --config vite.config.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "rimraf dist && pnpm build:web && pnpm build:server",
    "start": "node dist/server/index.js",
    "test": "jest --passWithNoTests --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "sync:nerds": "tsx src/server/jobs/manualSync.ts dynasty-nerds",
    "sync:calc": "tsx src/server/jobs/manualSync.ts dynasty-calculator"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino": "^9.4.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.2.2",
    "zod": "^3.23.8",
    "@prisma/client": "^6.16.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^2.0.16",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "daisyui": "^4.12.14",
    "postcss": "^8.4.47",
    "prisma": "^6.16.1",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "nock": "^13.5.5",
    "playwright": "^1.55.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1"
  }
}
